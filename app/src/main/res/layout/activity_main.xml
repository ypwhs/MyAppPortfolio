<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.yangpeiwen.myappportfolio.MainA">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:id="@+id/textView" />

    <Button
        android:text="@string/nnplayground"
        android:id="@+id/button"
        android:layout_below="@+id/textView"
        style="@style/MyButton" />

    <Button
        android:text="@string/raspberry_pi_ncamera"
        android:id="@+id/button2"
        android:layout_below="@+id/button"
        style="@style/MyButton" />

    <Button
        android:text="@string/instabtbu"
        android:id="@+id/button3"
        android:layout_below="@+id/button2"
        style="@style/MyButton" />

    <Button
        android:text="@string/laughornot"
        android:id="@+id/button4"
        android:layout_below="@+id/button3"
        style="@style/MyButton" />

    <Button
        android:text="@string/acceleration"
        android:id="@+id/button5"
        android:layout_below="@+id/button4"
        style="@style/MyButton" />

    <Button
        android:text="@string/arduino"
        android:id="@+id/button6"
        android:layout_below="@+id/button5"
        style="@style/MyButton" />

</RelativeLayout>
